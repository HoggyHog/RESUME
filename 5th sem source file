\section*{\LARGE \textbf{Data Science and Machine Learning}\xfilll[0pt]{0.5pt}}
\vspace{-10pt}
{\fontsize{12}{12} \textbf{The Humanoid Project $|$ Institute Technical Council, IIT Bombay}}  \hfill{\sl \small (Dec'22-Jan'22)}\\
\hspace{0pt}{ \textbf{Artificial Intelligence and Perception Subsystem}} 
\begin{itemize}[label=\textcolor{myblue}{\textbullet},itemsep = -1.25mm, leftmargin=5.5mm]
\vspace{-5pt}
\item\fontdimen1\font=0.5em Task was to read the labels given an image of many books which would be camptured by the humanoid.
\item\fontdimen1\font=0.5em \textls[5]{So first used \textbf{YOLOV8} to get crops of the different books within the image, and passed them down the pipeline.}
\item\fontdimen1\font=0.5em \textls[-20]{Trained model on \textbf{Roboflow} to  by making a \textbf{Custom Dataset} of the book and mannually annotating the labels}
\item\fontdimen1\font=0.5em \textls[-20]{From the labels, detected the words to be passed on using \textbf{EasyOCR} to perform Optical Character Reading}

\end{itemize}
\vspace{-3pt}
{\fontsize{12}{12} \textbf{DS303 Course Project $|$ Introduction to Machine Learning}}  \hfill{\sl \small (Mar'22 - Jun'22)}\\
\hspace{0pt}{\it Guide: Biplab Bannerjee  $|$ Center of Machine Learning and Data Science} \hfill{\it IIT Bombay}\
\begin{itemize}[label=\textcolor{myblue}{\textbullet},itemsep = -1.25mm, leftmargin=5.5mm]
\vspace{-8pt}
\item\fontdimen1\font=0.5em \textbf{Implemented a research paper} to analyse the significant features in models such as \textbf{RF and LightGBM}
\item\fontdimen1\font=0.5em \textls[5]{Used the \textbf{Iranian Customer Churn dataset}, with customer churn being the target variable for the model}
\item\fontdimen1\font=0.5em \textls[-20]{Used the frameworks of \textbf{SHAP and LIME} on the model created to understand which features were the most significant}
\item\fontdimen1\font=0.5em \textls[-20]{\textbf{Evaluation} was based on removing most significant features and noting its effect on the \textbf{Error metrics }(R^2,MSE,MAE) }

\end{itemize}
\vspace{-3pt}
{\fontsize{12}{12} \textbf{Facial Recognition using Siamese Networks $|$ Self- Project}}  \hfill{\sl \small (Dec'22-Jan'22)}\\
\begin{itemize}[label=\textcolor{myblue}{\textbullet},itemsep = -1.25mm, leftmargin=5.5mm]
\vspace{-17pt}
\item\fontdimen1\font=0.5em Task was to create a Facial Recognition Model using Contrastive Learning on positive and negative samples
\item\fontdimen1\font=0.5em \textls[5]{\textbf{Data pre-processing} involved collecting \textbf{1000+ images} and then using a \textbf{DataLoader} to create samples }
\item\fontdimen1\font=0.5em \textls[5]{Implemented Model architecture from the \textbf{Siamese Neural Networks paper}, based on \textbf{Contrastive Learning}}
\item\fontdimen1\font=0.5em \textls[-20]{For inference, input image was sampled against \textbf{50 validation images} to return a score compared with preset threshold}

\end{itemize}
\vspace{-3pt}
{\fontsize{12}{12} \textbf{Emotion Detector Using Deep Learning $|$ Winter In Data Science Project}}  \hfill{\sl \small (Dec'22-Jan'22)}\\
\begin{itemize}[label=\textcolor{myblue}{\textbullet},itemsep = -1.25mm, leftmargin=5.5mm]
\vspace{-17pt}
\item\fontdimen1\font=0.5em A dataset of about \textbf{35,000+ images from Kaggle's fer2013} were used for the training and testing of this Detector
\item\fontdimen1\font=0.5em \textls[5]{The input data was augmented using \textbf{Tensorflow and Keras functions} and then fed to the Model}
\item\fontdimen1\font=0.5em \textls[-20]{The training model had \textbf{Convolutional Neural Networks (CNN)} followed by batch Normalization and Max pooling}

\end{itemize}
\vspace{-3pt}
{\fontsize{12}{12} \textbf{Emotion Detector Using Deep Learning $|$ Winter In Data Science Project}}  \hfill{\sl \small (Dec'22-Jan'22)}\\
\begin{itemize}[label=\textcolor{myblue}{\textbullet},itemsep = -1.25mm, leftmargin=5.5mm]
\vspace{-17pt}
\item\fontdimen1\font=0.5em A dataset of about \textbf{35,000+ images from Kaggle's fer2013} were used for the training and testing of this Detector
\item\fontdimen1\font=0.5em \textls[5]{The input data was augmented using \textbf{Tensorflow and Keras functions} and then fed to the Model}
\item\fontdimen1\font=0.5em \textls[-20]{The training model had \textbf{Convolutional Neural Networks (CNN)} followed by batch Normalization and Max pooling}

\end{itemize}
